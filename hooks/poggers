	echo "       :+===x=++++++=====++++;;::+=========+;++++==++;:."
	echo "     :+====;;+======;;+;;;++++++++++;;=+;;+++==xxxxxxx=+;;."
	echo "    ;+====+======;;++=xX&########$X=+;:==;=$###########&&X=:"
	echo "   +++=========;+=;x###########$=;;+=Xx+;&##########=.   .+&x."
	echo "  ;++=========+++=###########&:       ;&+$#########;.= :+. :#$"
	echo " .+++=======;x&##############; ;$ +$X. =#;#########. : X#x .#&"
	echo " :+++=======+=xX&############;    =$X. =#;$########$:..   .$#x"
	echo " +++============++X###########; :.    +#X;++X&#######$xxx$#X;"
	echo ".+++============+==+x&#########&x=+=X#$;+====++=xxxXXXXx==+;"
	echo ".+++=============;;+=++=x$&######&$x=+;;======++;+++++++;:."
	echo ":+++==============x=+;++++++++++++==+;;================+;."
	echo ":+++======================++++++;;;+===========xx=====xx===+."
	echo ":+++===============================x==++++++++++++++++++++++:"
	echo ":+++==========================++++++==xxXXXXxx=========xXXXx."
	echo ":+++=====================++++=xXXXxx=+=======xXX$&&&&&&;xX+."
	echo ".++++================;;+=xXXx=+++++=xxxXXXXXXXXXX$&&&&==X:"
	echo " ++++================:XXX++;;+=xxxxxxxxXXXXXXXXXX$&&&==X="
	echo " ;+++===============x:Xx:+=====xxxxxxxxXXXXXXXXXX$&&&+xX="
	echo " :++++===============:x=:=====xxxxxxxXXXXXXXXXXX$&&&&=xX="
	echo "  +++++===============:X=;;+xxxxxxxxXXXXXXXXXXX$&&&&&x+Xx"
	echo "  .++++================;xXX+;xxxxxXXXXXXXXXXX$&&&&&&&$;xx"
	echo "   :++++================++xXx;=XXXXXXXXXXX$$&&&&&&&&&&:Xx"
	echo "    .+++++================;+XX=+=XXXXX$$$&&&&&&&&&&&&&;Xx"
	echo "      ;++++=================;=xXx++=$&&&&&&&&&&&&&&&&X;Xx"
	echo "       .;++++=================++=xXx===x$&&&&&&&$Xx===XXx"
	echo "         .;++++==================+++xXXX==+======xxXXXx=:"
	echo "           .:;++++==================+++++++====++++++;."
